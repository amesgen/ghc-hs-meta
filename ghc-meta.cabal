cabal-version: 1.12

name:           ghc-meta
version:        0.0.1
build-type:     Simple

library
  exposed-modules:
      GHC.Meta.Parser
      GHC.Meta.ParserEx
  other-modules:
      Paths_ghc_meta
  hs-source-dirs:
      src
  default-extensions:
      TemplateHaskell
      LambdaCase
  ghc-options: -Wall
  build-depends:
      base >=4.14 && <4.17
    , ghc
    , ghc-boot
    , template-haskell
  default-language: Haskell2010
